<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- ================================================================== -->
    <!--  dubbo provider configs: auto generate by dalgen  不要手动修改本文件 -->
    <!-- 本文件由dalgen生成，任何在本文件上的手工修改将会在下次生成时被覆盖===== -->
    <!-- ================================================================== -->
    
    <dubbo:service timeout="10000" retries="0" registry="${systemName}-reg-addr" ref="menuService" interface="${packageName}.service.MenuService"/>
    <dubbo:service timeout="10000" retries="0" registry="${systemName}-reg-addr" ref="roleService" interface="${packageName}.service.RoleService"/>
    <dubbo:service timeout="10000" retries="0" registry="${systemName}-reg-addr" ref="roleMenuService" interface="${packageName}.service.RoleMenuService"/>
    <dubbo:service timeout="10000" retries="0" registry="${systemName}-reg-addr" ref="userService" interface="${packageName}.service.UserService"/>
    <dubbo:service timeout="10000" retries="0" registry="${systemName}-reg-addr" ref="userRoleService" interface="${packageName}.service.UserRoleService"/>
	
</beans>